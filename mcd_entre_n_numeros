#include <iostream>
#include <limits>

using namespace std;

int main() {
    int N, cant, mcd, r;
    
    do {
        cout << "Ingresa la cantidad de números: ";
        cin >> cant;
    } while (cant <= 0);

    cout << "Ingrese el número 1: ";
    cin >> N;
    mcd = N; 

    for (int i = 1; i < cant; i++) {
        cout << "Ingrese el número " << i + 1 << " : ";
        cin >> N;
        
        int a = mcd, b = N;
        while (b != 0) {
            r=a%b;
            a=b;
            b=r;
        }
        mcd = a;
    }
    cout << "El MCD de los " << cant << " números es: " << mcd << endl;

    return 0;
}
